		<apparel>
			<developmentalStageFilter>Child, Adult</developmentalStageFilter>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>Medieval</li>
			</tags>
		</apparel>
		<colorGenerator Class="ColorGenerator_StandardApparel" />
		<tradeTags>
			<li>Clothing</li>
		</tradeTags>
	</ThingDef>

